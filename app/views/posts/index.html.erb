<h1>Bienvenidos</h1>

<% if notice %>
    <%= notice %>
<% end %>



<% @posts.each do |post| %>
<div>
    <h3><%= post.title %></h3>
        <p><%= post.created_at.strftime("%d.%m.%Y") %></p>
        <p>
        <%= image_tag(post.image_url)%>
        </p>
        <p><%= post.content %></p>
</div>
<% end %>